{
    "rewrites": [
        {
            "source": "/finances/(.*)",
            "destination": "/finances/index.html"
        },
        {
            "source": "/finances/",
            "destination": "/index.html"
        },
        {
            "source": "/finances/:path((?!.*\\.).*)",
            "destination": "/index.html"
        }
    ]
}