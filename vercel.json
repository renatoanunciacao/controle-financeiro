{
    "builds": [
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/finances/(.*)",
        "destination": "/finances/index.html"
      },
      {
        "source": "/finances/*",
        "destination": "/dist/finances/index.html"
      }
    ]
  }
  